    <title>3rd space sharing</title>
</head>

<body >
    <!-- NAVIGATION BAR START -->
    <%- include("nav") %>
        <!-- NAVIGATION BAR END-->
        <!-- IMAGE CAROUSEL START-->
    <!-- IMAGE CAROUSEL END-->
    <!-- OPENING PARAGRAPH-->
    <div class="container">
        <div class="row">
            <%for (var i=0; i < images.length ; i++) { %>
                <div class="col-4">
                    <div>Published by: <%= images[i].username %></div>
                    <div>Picture Date: <%= images[i].date %></div>
                    <img src="data:image/jpg;base64<%= images[i].src %>" width=250px><br>
                </div>
                <% } %>
        </div>
    </div>
    <!--LIST OF EVENT START-->
</body>

</html>